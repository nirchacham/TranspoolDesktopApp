<?xml version="1.0" encoding="UTF-8"?>
<TransPool xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="transpool.xsd">
	<MapDescriptor>
		<MapBoundries length="10" width="10"/>
		<Stops>
			<Stop name="A" x="3" y="4"/>
			<Stop name="B" x="1" y="2"/>
			<Stop name="C" x="7" y="3"/>
		</Stops>
		<Paths>
			<Path from="A" to="B">
				<Length>20</Length>
				<FuelConsumption>10</FuelConsumption>
				<SpeedLimit>80</SpeedLimit>
			</Path>
			<Path from="B" to="C" one-way="true">
				<Length>15</Length>
				<FuelConsumption>5</FuelConsumption>
				<SpeedLimit>40</SpeedLimit>
			</Path>
		</Paths>
	</MapDescriptor>
	<PlannedTrips>
		<TransPoolTrip>
			<Owner>Mosh</Owner>
			<Capacity>3</Capacity>
			<PPK>20</PPK>
			<Route path="A,B,C"/>
			<Scheduling hour-start="10" day-start="2" recurrences="Daily"/>
		</TransPoolTrip>
	</PlannedTrips>
</TransPool>
